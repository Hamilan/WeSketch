package sketching.widgets;

/**
 * This class represents the ConfirmDialog widget to use in the Sketching system.
 * Objects of this class are sendable through the net with Kryonet.
 * @author Hamilan
 */
public class ConfirmDialog extends MessageDialog {
	transient public final static int YESNO = 0; 
	transient public final static int YESNOCANCEL = 1; 
	transient public final static int SINO = 2; 
	transient public final static int SINOCANCELAR = 3; 
	/**
	 * Based on javax.Swing.JOptionPane constants. Takes a value among the constants defined in this class.
	 * data.type : buttonsSet;  
	 */
	public ConfirmDialog() {
		this.data.widgetType="ConfirmDialog";
	}
}
